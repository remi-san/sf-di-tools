{
    "name": "remi-san/sf-di-tools",
    "description" : "A set of tools for symfony DI",
    "license" : "MIT",
    "keywords": [
        "Symfony Dependency Injection",
        "Tools"
    ],
    "authors": [
        {
            "name": "RÃ©mi San",
            "email": "remi.san@gmail.com",
            "homepage": "http://remi-san.github.io/",
            "role": "Developer"
        }
    ],
    "require": {
        "symfony/dependency-injection": "^3.0",
        "symfony/config" : "^3.0",
        "symfony/yaml" : "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5",
        "Mockery/Mockery": "^0.9",
        "fabpot/php-cs-fixer": "^1.10",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Component\\DependencyInjection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Component\\DependencyInjection\\Test\\": "tests/unit",
            "Cache\\": "tests/unit/cache"
        }
    }
}
